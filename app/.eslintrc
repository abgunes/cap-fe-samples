{
	"env": {
		"browser": true
	},
	"ignorePatterns": [
		"test/",
		"node_modules/",
		"localService/",
		"dist/",
		"resources/",
		"Gruntfile.js"
	],
	"plugins": [
		"jsdoc",
		"ui5"
	],
	"extends": "eslint:recommended",
	"rules": {
		"ui5/no-global-id": 1,
		"ui5/no-global-name": 1,
		"ui5/no-for-in": 1,
		"ui5/no-boolean-literal-compare": 1,
		"ui5/hungarian-notation": [
			1,
			{
				"ignore": [
					"models",
					"formatter",
					"mobileLibrary"
				]
			}
		],
		"jsdoc/check-alignment": 1,
		"jsdoc/check-examples": 1,
		"jsdoc/check-indentation": 1,
		"jsdoc/check-param-names": 1,
		"jsdoc/check-syntax": 1,
		"jsdoc/check-tag-names": 1,
		"jsdoc/check-types": 1,
		"jsdoc/newline-after-description": 0,
		"jsdoc/no-undefined-types": 1,
		"jsdoc/require-description": 0,
		"jsdoc/require-description-complete-sentence": 1,
		"jsdoc/require-example": 0,
		"jsdoc/require-hyphen-before-param-description": 0,
		"jsdoc/require-param": 1,
		"jsdoc/require-param-description": 1,
		"jsdoc/require-param-name": 1,
		"jsdoc/require-param-type": 1,
		"jsdoc/require-returns": 1,
		"jsdoc/require-returns-check": 1,
		"jsdoc/require-returns-description": 1,
		"jsdoc/require-returns-type": 1,
		"jsdoc/valid-types": 1,
		"array-callback-return": 2,
		"block-scoped-var": 2,
		"brace-style": [
			1,
			"1tbs"
		],
		"camelcase": [
			"error",
			{
				"allow": [
					"_Id$"
				]
			}
		],
		"complexity": [
			2,
			11
		],
		"consistent-return": 1,
		"consistent-this": [
			0,
			"that"
		],
		"curly": [
			0,
			"all"
		],
		"default-case": 0,
		"dot-notation": [
			0,
			{
				"allowKeywords": true
			}
		],
		"eqeqeq": 1,
		"func-names": 0,
		"func-style": [
			0,
			"declaration"
		],
		"guard-for-in": 0,
		"handle-callback-err": 0,
		"keyword-spacing": [
			1,
			{
				"before": true,
				"after": true
			}
		],
		"max-depth": [
			0,
			4
		],
		"max-len": [
			0,
			80,
			4
		],
		"max-nested-callbacks": [
			0,
			2
		],
		"max-params": [
			0,
			3
		],
		"max-statements": [
			0,
			10
		],
		"new-cap": 1,
		"new-parens": 2,
		"no-alert": 1,
		"no-array-constructor": 1,
		"no-bitwise": 0,
		"no-caller": 2,
		"no-catch-shadow": 1,
		"no-cond-assign": 1,
		"no-console": 1,
		"no-constant-condition": 2,
		"no-control-regex": 1,
		"no-debugger": 2,
		"no-delete-var": 1,
		"no-div-regex": 0,
		"no-dupe-keys": 2,
		"no-else-return": 0,
		"no-empty": 2,
		"no-eq-null": 0,
		"no-eval": 2,
		"no-ex-assign": 2,
		"no-extend-native": 1,
		"no-extra-boolean-cast": 2,
		"no-extra-parens": 0,
		"no-extra-semi": 2,
		"no-fallthrough": 1,
		"no-floating-decimal": 0,
		"no-func-assign": 2,
		"no-implied-eval": 1,
		"no-inner-declarations": [
			0,
			"functions"
		],
		"no-invalid-regexp": 2,
		"no-irregular-whitespace": 2,
		"no-iterator": 1,
		"no-label-var": 1,
		"no-labels": 1,
		"no-lone-blocks": 1,
		"no-lonely-if": 0,
		"no-loop-func": 1,
		"no-mixed-requires": [
			0,
			false
		],
		"no-mixed-spaces-and-tabs": [
			0,
			false
		],
		"no-multi-str": 1,
		"no-multiple-empty-lines": [
			0,
			{
				"max": 2
			}
		],
		"no-native-reassign": 2,
		"no-negated-in-lhs": 2,
		"no-nested-ternary": 0,
		"no-new": 1,
		"no-new-func": 2,
		"no-new-object": 1,
		"no-new-require": 0,
		"no-new-wrappers": 1,
		"no-obj-calls": 2,
		"no-octal": 1,
		"no-octal-escape": 1,
		"no-path-concat": 0,
		"no-plusplus": 0,
		"no-process-exit": 1,
		"no-proto": 1,
		"no-redeclare": 1,
		"no-regex-spaces": 1,
		"no-restricted-modules": 0,
		"no-return-assign": 1,
		"no-script-url": 1,
		"no-self-compare": 0,
		"no-sequences": 1,
		"no-shadow": 1,
		"no-shadow-restricted-names": 1,
		"no-spaced-func": 1,
		"no-sparse-arrays": 2,
		"no-sync": 0,
		"no-ternary": 0,
		"no-undef": 2,
		"no-undef-init": 1,
		"no-underscore-dangle": 0,
		"no-unreachable": 2,
		"no-unused-expressions": 1,
		"no-unused-vars": [
			1,
			{
				"vars": "all",
				"args": "after-used"
			}
		],
		"no-use-before-define": 1,
		"no-warning-comments": [
			0,
			{
				"terms": [
					"todo",
					"fixme",
					"xxx"
				],
				"location": "start"
			}
		],
		"no-with": 2,
		"yoda": [
			2,
			"never"
		],
		"one-var": 0,
		"quote-props": 0,
		"quotes": [
			1,
			"double"
		],
		"radix": 0,
		"semi": 2,
		"sort-vars": 0,
		"space-before-blocks": [
			1,
			"always"
		],
		"space-infix-ops": 0,
		"space-unary-ops": [
			1,
			{
				"words": true,
				"nonwords": false
			}
		],
		"strict": 0,
		"use-isnan": 2,
		"valid-jsdoc": 0,
		"valid-typeof": 1,
		"wrap-iife": 0,
		"wrap-regex": 0,
		"no-dupe-args": 2,
		"no-duplicate-case": 2,
		"no-extra-bind": 1,
		"no-inline-comments": 0,
		"no-multi-spaces": [
			1,
			{
				"ignoreEOLComments": true
			}
		],
		"no-process-env": 0,
		"no-trailing-spaces": 0,
		"no-throw-literal": 1,
		"no-undefined": 0,
		"no-void": 1,
		"no-var": 0,
		"comma-dangle": [
			0,
			"never"
		],
		"comma-spacing": 0,
		"comma-style": 0,
		"eol-last": 0,
		"generator-star-spacing": 0,
		"indent": 0,
		"key-spacing": [
			0,
			{
				"beforeColon": false,
				"afterColon": true
			}
		],
		"operator-assignment": [
			0,
			"always"
		],
		"padded-blocks": 0,
		"semi-spacing": [
			0,
			{
				"before": false,
				"after": true
			}
		],
		"space-before-function-paren": [
			0,
			"never"
		],
		"space-in-parens": [
			0,
			"never"
		],
		"vars-on-top": 0,
		"no-continue": 0,
		"no-empty-character-class": 0,
		"linebreak-style": [
			0,
			"unix"
		],
		"no-param-reassign": 0,
		"no-unneeded-ternary": 0,
		"prefer-const": 0,
		"array-bracket-spacing": [
			0,
			"never"
		],
		"accessor-pairs": 0,
		"computed-property-spacing": [
			0,
			"never"
		],
		"dot-location": 0,
		"lines-around-comment": 0,
		"newline-after-var": 0,
		"object-curly-spacing": [
			0,
			"never"
		],
		"object-shorthand": 0,
		"operator-linebreak": 0,
		"spaced-comment": [
			1,
			"always",
			{
				"exceptions": [
					"-"
				]
			}
		]
	},
	"globals": {
		"sap": false,
		"jQuery": false,
		"Promise": false
	}
}